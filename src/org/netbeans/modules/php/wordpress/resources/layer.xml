<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-php5">
                <folder name="CodeTemplates">
                    <file name="org-netbeans-modules-phpeditor-snippets.xml" url="org-netbeans-modules-phpeditor-snippets.xml"/>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsExport">
        <folder name="PHP">
            <file name="WordPress">
                <attr name="include" stringvalue="config/Preferences/org/netbeans/modules/php/wordpress/.*"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.php.wordpress.resources.Bundle#LBL_WordPressExportName"/>
                <attr name="position" intvalue="1500"/>
            </file>
        </folder>
    </folder>
    <folder name="org-netbeans-modules-php-wordpress">
        <file name="WpPlugin.php" url="WpPlugin.php" />
        <file name="readme.txt" url="readme.txt" />
    </folder>
</filesystem>
